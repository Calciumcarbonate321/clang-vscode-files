    "version": "2.0.0",
    "tasks": [
      {
        "type": "cppbuild",
        "label": "build",
        "command": "clang-12",
        "args": [
          "-Wall",
          "-Wextra",
          "${file}",
          "-o",
          "${fileDirname}/bin/${fileBasenameNoExtension}"
        ],
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "detail": "Task generated by Debugger."
      }
    ]
  }
  
